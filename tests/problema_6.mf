import io

def fun void main()
    int[] arvore = [50, 30, 70, 20, 40, 60, 80, 10, 25, 35, 45]

    print("Arvore binaria de busca (valores ordenados):")

    int i = 0
    while (i < 11)
        print(arvore[i])
        i = i + 1

    int minimo = arvore[0]
    int j = 1
    while (j < 11)
        if (arvore[j] < minimo)
            minimo = arvore[j]
        j = j + 1

    print("Valor minimo:")
    print(minimo)

    int maximo = arvore[0]
    int k = 1
    while (k < 11)
        if (arvore[k] > maximo)
            maximo = arvore[k]
        k = k + 1

    print("Valor maximo:")
    print(maximo)

    print("Busca concluida")
end def

