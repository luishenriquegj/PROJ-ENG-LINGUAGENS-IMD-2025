import math

def fun int fibonacci(int n)
    if (n <= 1)
        return n

    return fibonacci(n - 1) + fibonacci(n - 2)
end def

def fun float calcular_media(float[] notas)
    float soma = 0.0
    int count = 0

    for (nota in notas)
        soma += nota
        count++

    return soma / count
end def

