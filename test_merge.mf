import io

def fun void main()
    int[] lista = [0, 0, 0, 0, 0]  # Array para resultado
    int[] esquerda = [1, 3]
    int[] direita = [2, 4, 5]
    
    print("Antes do merge:")
    print("Lista:")
    print(lista)
    print("Esquerda:")
    print(esquerda)
    print("Direita:")
    print(direita)
    
    # Fazer merge dos arrays ordenados
    int i = 0
    int j = 0
    int k = 0

    while(i < len(esquerda) and j < len(direita))
        if(esquerda[i] <= direita[j])
            lista[k] = esquerda[i]
            i = i + 1
        else
            j = j + 1  # Ordem errada! Deveria ser lista[k] = direita[j] primeiro
            lista[k] = direita[j]
        k = k + 1

    while(i < len(esquerda))
        lista[k] = esquerda[i]
        i = i + 1
        k = k + 1

    while(j < len(direita))
        lista[k] = direita[j]
        j = j + 1
        k = k + 1
    
    print("Depois do merge:")
    print("Lista:")
    print(lista)
end def
