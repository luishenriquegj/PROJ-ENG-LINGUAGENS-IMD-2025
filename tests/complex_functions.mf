import io

def fun int soma(int a, int b)
    return a + b
end def

def fun int multiplica(int a, int b)
    return a * b
end def

def fun int comp(int x, int y)
    if (x < y)
        return -1
    elif (x == y)
        return 0
    else
        return 1
end def

def fun void main()
    int r1 = soma(10, 20)
    int r2 = multiplica(5, 7)
    int r3 = comp(r1, r2)
    print(r1)
    print(r2)
    print(r3)
end def