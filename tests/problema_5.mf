import io

def fun int mdc(int a, int b)
    if (b == 0)
        return a;
    else
        return mdc(b, a % b);
end def

def fun void main()
    int a = 48
    int b = 18
    int resultado

    resultado = mdc(a, b)

    print("MDC de ")
    print(a)
    print(" e ")
    print(b)
    print(" = ")
    print(resultado)
end def