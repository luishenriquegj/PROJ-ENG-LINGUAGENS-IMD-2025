import io

def fun void mdc(int n, int m, ref int r)
    if (m % n == 0)
        r = n
        return

    if (n % m == 0)
        r = m
        return

    if (m > n)
        int resto = m % n
        mdc(n, resto, r)
    else
        int resto = n % m
        mdc(m, resto, r)

end def

def fun void main()
    int a = 48
    int b = 18
    int resultado

    mdc(a, b, ref resultado)

    print("MDC de ")
    print(a)
    print(" e ")
    print(b)
    print(" = ")
    print(resultado)
    
end def


