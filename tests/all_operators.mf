import io

def fun void main()
    # Teste de todos os operadores de atribuição
    int x = 10
    print(x)

    x += 5
    print(x)

    x -= 3
    print(x)

    x *= 2
    print(x)

    x /= 4
    print(x)

    x %= 5
    print(x)

    # Teste de incremento/decremento
    int y = 0
    y++
    print(y)

    ++y
    print(y)

    y--
    print(y)

    --y
    print(y)

    # Teste de operadores lógicos alternativos
    bool a = true
    bool b = false

    bool c = a && b
    print(c)

    bool d = a || b
    print(d)

    bool e = !a
    print(e)

    print("Todos os operadores testados")
end def