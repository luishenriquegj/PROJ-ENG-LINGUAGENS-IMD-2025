import io

def fun void main()
    # String + int (incompatível)
    string msg = "numero: "
    int num = 42
    string resultado1 = msg + num
    
    # Bool + int (incompatível)
    bool flag = true
    int x = 10
    int resultado2 = flag + x
    
    # Complex + string (incompatível)
    complex z = 3+4i
    string texto = "complexo"
    complex resultado3 = z + texto
    
    # Array de int em atribuição de float
    int[] arr_int = [1, 2, 3]
    float[] arr_float = arr_int
    
    # Comparação de tipos incompatíveis
    bool comp1 = "texto" > 42
    bool comp2 = true == "true"
    
    # Operações lógicas com tipos não-booleanos
    int a = 5
    int b = 10
    bool resultado4 = a and b
    
    # Atribuição de array a escalar
    int[] numeros = [1, 2, 3]
    int escalar = numeros
    
    # Operação matemática com string
    string s = "10"
    int resultado5 = s * 2
    
    # Indexação de não-array
    int n = 42
    int elemento = n[0]
    
    # Chamada de função com tipo errado
    float f = 3.14
    teste_int(f)
end def

def fun void teste_int(int valor)
    print(valor)
end def